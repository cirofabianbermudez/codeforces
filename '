#include <bits/stdc++.h>
using namespace std;

#define ll long long

void toUpper(string &str){
  int l = str.length();
  for (int i = 0; i < l; i++){
    if (str[i] > 90) {
      str[i] -= 32;
    }
  }
}


int main() {
  ios::sync_with_stdio(0);
  cin.tie(0);
  
  string s1, s2;
  cin >> s1 >> s2;

  int l;
  l = s1.length();

  int c = 0;
  for (int i = 0; i < l; i++) {
    toUpper(s1);
    toUpper(s2);
    if (s1[i] > s2[i]) {
      cout << 1;
    } else if (s1[i] < s2[i]){
      cout << -1;
    }
  }
  cout << 0;
  return 0;
}
